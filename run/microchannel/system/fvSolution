FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1e-6;
        relTol          0.01;
        //maxIter         200;
        //maxIter         100000;
        //minIter         200000;
    }

    "(U|k|omega|e)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0.1;
        minIter 2;
    }
}

SIMPLE
{
    residualControl
    {
        p               1e-6;
        U               1e-6;
        "(k|omega|e)"   1e-6;
    }

    nNonOrthogonalCorrectors 0;
    pMinFactor      0.1;
    pMaxFactor      2;

    transonic   yes;
    consistent  yes;
    
}

relaxationFactors
{
    fields
    {
        //p               0.001;//0.7;
        rho             0.0001;
    }
    equations
    {
        //p   0.5;        
        U               0.3;
        e               0.7;
        "(k|omega)"     0.7;
    }
}

