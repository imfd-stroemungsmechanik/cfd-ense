FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1e-6;
        relTol          0.01;
        minIter 2;
        //maxIter         200;
        //maxIter         100000;
        //minIter         200000;
    }

    "(Uc|k|omega|e)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-6;
        relTol          0.1;
        minIter 2;
    }
}

SIMPLE
{
    residualControl
    {
        p               1e-4;
        Uc               1e-4;
        "(k|omega|e)"   1e-4;
    }

    nNonOrthogonalCorrectors 0;
    pMinFactor      0.1;
    pMaxFactor      2;

    transonic   no;
    consistent  no;

    isothermal yes;    
}

relaxationFactors
{
    fields
    {
        // rhoSimpleEnseFoam/rhoSimpleFoam(without EEqn):
        p   0.3;
        
        // rhoSimpleEnseFoam (full explicit):
        //p               0.0002;

        // rhoSimpleFoam(without EEqn):
        //rho             0.01;
    }
    equations
    {                
        Uc               0.7;
        e               0.7;
        "(k|omega)"     0.7;
    }
}

